    <div class="row espacio">
        <div class="large-6 column text-center boton_tipo_user_tombola btn_activo_user_tombola user_old">
            <h3>
                YA ES USUARIO
            </h3>
        </div>
        <div class="large-6 column text-center boton_tipo_user_tombola user_new">
            <h3 style="opacity:1;">
                USUARIO NUEVO
            </h3>
        </div>
    </div>
    <div class="row espacio campos_user_old">
        <div class="large-6 column large-centered">
            <label class="naranja">
                Cedula
            </label>
            <input id="cedula_old" class="input" type="text" placeholder="Cedula Usuario">
        </div>
    </div>

    <div class="campos_user_new">
        <div class="row espacio ">
            <div class="large-6 column large-centered">
                <label class="naranja">
                    Nombre
                </label>
                <input id="nombre_new" class="input" type="text" placeholder="Cedula Usuario">
            </div>
        </div>
        <div class="row espacio ">
            <div class="large-6 column large-centered">
                <label class="naranja">
                    Correo
                </label>
                <input id="correo_new" class="input" type="text" placeholder="Cedula Usuario">
            </div>
        </div>
        <div class="row espacio ">
            <div class="large-6 column large-centered">
                <label class="naranja">
                    Cedula
                </label>
                <input id="cedula_new" class="input" type="text" placeholder="Cedula Usuario">
            </div>
        </div>
        <div class="row espacio ">
            <div class="large-6 column large-centered">
                <label class="naranja">
                    Telefono
                </label>
                <input id="telefono_new" class="input" type="text" placeholder="Cedula Usuario">
            </div>
        </div>
    </div>


    <div class="row espacio">
        <div class="large-6 column large-centered">
            <label class="naranja">
                Cedula Vendedor
            </label>
            <input id="cedula_vendedor" class="input" type="text" placeholder="Cedula Vendedor">
        </div>
    </div>
    <div class="row espacio">
        <!--<img src="/images/fondo-tombola.jpg" id="img-chambon"/>-->

        <div class="large-6 column large-centered">
            <label class="naranja">
                Seleccionar el carro
            </label>
            <select id="carro_compro" class="input select_naranja">
                <option value=""></option>
                <option value="SPARK LIFE SIN AIRE">SPARK LIFE Sin Aire</option>
                <option value="SPARK LIFE CON AIRE">SPARK LIFE Con Aire</option>

                <option value="NUEVO SPARK GT LT con Aire">NUEVO SPARK GT LT con Aire</option>
                <option value="NUEVO SPARK GT LTZ (FULL)">NUEVO SPARK GT LTZ (FULL)</option>
                <option value="NUEVO SPARK GT LT con Aire - Versión RS">NUEVO SPARK GT LT con Aire - Versión RS</option>
                <option value="NUEVO SPARK GT LTZ (FULL) - Versión RS">NUEVO SPARK GT LTZ (FULL) - Versión RS</option>

                <option value="SAIL SEDAN LS sin aire">SAIL SEDAN LS sin aire</option>
                <option value="SAIL SEDAN LS con aire">SAIL SEDAN LS con aire</option>
                <option value="SAIL SEDAN LT">SAIL SEDAN LT</option>
                <option value="SAIL SEDAN LTZ">SAIL SEDAN LTZ</option>

