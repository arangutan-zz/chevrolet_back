  <body class="body_tombola">

    <header>
        <div class="row">
            <div class="large-2 column">
                <img src="/images/logo_chevrolet.png" class="espacio_titulo">
            </div>
            <div class="large-9 large-offset-1 column ">
                <h1 class="title_app espacio_titulo">REGISTRO DE COMPRA</h1>
            </div>
        </div>
    </header>
