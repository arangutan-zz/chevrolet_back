                <option value="SONIC HATCHBACK LT Mecánico">SONIC HATCHBACK LT Mecánico</option>
                <option value="SONIC HATCHBACK LT Automático">SONIC HATCHBACK LT Automático</option>
                <option value="SONIC HATCHBACK LT Mecánico - Versión RS">SONIC HATCHBACK LT Mecánico - Versión RS</option>
                <option value="SONIC HATCHBACK LT Automático - Versión RS">SONIC HATCHBACK LT Automático - Versión RS</option>

                <option value="COBALT LTZ Mecánico">COBALT LTZ Mecánico</option>
                <option value="COBALT LTZ Automático">COBALT LTZ Automático</option>

                <option value="CRUZE SEDÁN LS Mecánico">CRUZE SEDÁN LS Mecánico</option>
                <option value="CRUZE SEDÁN LS Automático">CRUZE SEDÁN LS Automático</option>
                <option value="CRUZE SEDÁN LT Automático">CRUZE SEDÁN LT Automático</option>

                <option value="CRUZE HATCHBACK LT Mecánico">CRUZE HATCHBACK LT Mecánico</option>
                <option value="CRUZE HATCHBACK LT Automático">CRUZE HATCHBACK LT Automático</option>

                <option value="CAMARO Versión LZ1">CAMARO Versión LZ1</option>

                <option value="TRACKER LS Mecánica">TRACKER LS Mecánica</option>
                <option value="TRACKER LS Automática">TRACKER LS Automática</option>
                <option value="TRACKER LT Automática">TRACKER LT Automática</option>

                <option value="CAPTIVA SPORT LS Automática">CAPTIVA SPORT LS Automática</option>
                <option value="CAPTIVA SPORT LS Automática con Sunroof">CAPTIVA SPORT LS Automática con Sunroof</option>
                <option value="CAPTIVA SPORT LT Automática">CAPTIVA SPORT LT Automática</option>

                <option value="ORLANDO LT Automática">ORLANDO LT Automática</option>

                <option value="NUEVA TRAVERSE LT Automática">NUEVA TRAVERSE LT Automática</option>

                <option value="TRAILBLAZER LT Mecánica">TRAILBLAZER LT Mecánica</option>
                <option value="TRAILBLAZER LTZ Automática">TRAILBLAZER LTZ Automática</option>

                <option value="TAHOE LT Automática">TAHOE LT Automática</option>
                <option value="TAHOE ELEGANCE 5.3 L LT Automática - ELEGANCE">TAHOE ELEGANCE 5.3 L LT Automática - ELEGANCE</option>

                <option value="N300 MAX CARGO Mecánica">N300 MAX CARGO Mecánica</option>
                <option value="N300 MAX CARGO Mecánica PLUS">N300 MAX CARGO Mecánica PLUS</option>

                <option value="N300 MOVE PASAJEROS Mecánica">N300 MOVE PASAJEROS Mecánica</option>
                <option value="N300 MOVE PASAJEROS Mecánica PLUS">N300 MOVE PASAJEROS Mecánica PLUS</option>

                <option value="DMAX Diesel 4x2 Cabina Sencilla">DMAX Diesel 4x2 Cabina Sencilla</option>
                <option value="DMAX Diesel 4x2 Cabina Doble">DMAX Diesel 4x2 Cabina Doble</option>
                <option value="DMAX Diesel 4x4 Cabina Sencilla">DMAX Diesel 4x4 Cabina Sencilla</option>
                <option value="DMAX Diesel 4x4 Cabina Doble">DMAX Diesel 4x4 Cabina Doble</option>
                <option value="DMAX Diesel 4x4 Cabina Doble FULL">DMAX Diesel 4x4 Cabina Doble FULL</option>

                <option value="TAXI 7:24 Sin Aire">TAXI 7:24 Sin Aire</option>
                <option value="TAXI 7:24 Con Aire">TAXI 7:24 Con Aire</option>

                <option value="TAXI ELITE LS con aire">TAXI ELITE LS con aire</option>
            </select>
