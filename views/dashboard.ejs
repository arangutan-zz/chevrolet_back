    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body ng-app="Dashboard" class="dashbody">
    <div class="datos_now db" ng-controller="DatosNowCtrl">
      <div class="row" >
        <div class="large-4 column">
          <h6 class="fecha text-center bold">{{data.date}}</h6>
          <h5 class="text-center mayusculas titulo-dashboard">Personas que han entrado hoy</h5>
          <hr class="bg-negro">
          <p class="dato_dashboard text-center">
            {{data.in_total}}
          </p>
        </div>
<!--
       <div class="large-4 column">
