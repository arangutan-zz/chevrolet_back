-->

	<div class="large-5 column">
		<h6 class="fecha text-center bold">{{data.date}}</h6>
		<h5 class="text-center mayusculas titulo-dashboard">Carro más consultado</h5>
		<hr class="bg-negro">

		<ul ng-repeat="car in cars">
			<li>{{car._id | uppercase}} - {{car.total_sells}}</li>
		</ul>
		<!--<p class="text-center">
			{{data.car_call.name}}
		</p>-->
	</div>
        <div class="large-3 column">
          <h6 class="fecha text-center bold">{{data.date}}</h6>
          <h6 class="text-center mayusculas titulo-dashboard">Veces que han llamado al vendedor</h6>
          <hr class="bg-negro">
          <p class="dato_dashboard text-center">
            {{data.calls}}
          </p>

  <p>
      <div>
          Efectividad sobre entradas
      </div>
      <div>
        {{(data.calls*100)/data.in_total}} %
      </div>
    
    </p>

        </div>
      </div>
      <br>
      <br>
      <div class="row" >
        <div class="large-4 column">
          <h6 class="fecha text-center bold">{{data.date}}</h6>
          <h5 class="text-center mayusculas titulo-dashboard">Ventas realizadas</h5>
          <hr class="bg-negro">
          <p class="dato_dashboard text-center">
            {{data.sells}}
          </p>

          <p>
          <div>
              Efectividad sobre entradas
          </div>
          <div>
            {{(data.sells*100)/data.in_total}} %
          </div>
          <br>
        </p>
	       <p>
          <div>
            Efectividad sobre llamadas
          </div>
          <div>
            {{(data.sells*100)/data.calls}} %
          </div>		
	       </p>



        </div>


        <div class="large-4 column">
          <h6 class="text-center mayusculas titulo-dashboard">VENDEDOR CON MÁS VENTAS</h6>
          <hr class="bg-negro">
          <ul ng-repeat="seller in sellers">
            <li>{{seller.name}} - {{seller.units}}</li>
          </ul>
        </div>
	<div class="large-4 column">
		<h6 class="text-center mayusculas titulo-dashboard">CARRO MÁS VENDIDO</h6>
		<hr class="bg-negro">
		<ul ng-repeat="car_sell in car_sells">
			<li>{{car_sell._id | uppercase}} - {{car_sell.total_sells}}</li>
		</ul>
	</div>

